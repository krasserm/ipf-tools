<?eclipse version="3.2"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/computer.png"
            thread="main">
         <run
               class="org.openehealth.ipf.tools.manager.application.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.openehealth.ipf.tools.manager.application.application"
            name="IPF Manager">
         <property
               name="appName"
               value="IPF Manager">
         </property>
         <property
               name="windowImages"
               value="icons/computer.png">
         </property>
         <property
               name="aboutText"
               value="{0}">
         </property>
         <property
               name="aboutImage"
               value="icons/oehf.png">
         </property>
      </product>
   </extension>

</plugin>
